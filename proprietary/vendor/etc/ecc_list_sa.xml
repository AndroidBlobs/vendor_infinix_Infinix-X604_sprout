<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Saudi arabia
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="999" Category="0" Condition="1" Name="Police"/>
    <EccEntry Ecc="933" Category="0" Condition="1" Name="Electricity"/>
    <EccEntry Ecc="993" Category="0" Condition="1" Name="Traffic Police"/>
    <EccEntry Ecc="996" Category="0" Condition="1" Name="Highway Patrol"/>
    <EccEntry Ecc="997" Category="0" Condition="1" Name="Ambulance"/>
    <EccEntry Ecc="998" Category="0" Condition="1" Name="Fire"/>
</EccTable>
