<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!-- 
        Papua New Guinea
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="000" Category="0" Condition="2" Name="Emergency Number"/>
    <EccEntry Ecc="110" Category="0" Condition="2" Name="Emergency Number"/>
    <EccEntry Ecc="111" Category="0" Condition="2" Name="Emergency Number"/>
</EccTable>
