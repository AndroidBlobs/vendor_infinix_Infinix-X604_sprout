<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Malawi
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="911" Category="0" Condition="2" Name="Police" />
    <EccEntry Ecc="997" Category="0" Condition="2" Name="Police" />
    <EccEntry Ecc="999" Category="0" Condition="2" Name="Fire" />
    <EccEntry Ecc="990" Category="0" Condition="2" Name="City Emergency" />
    <EccEntry Ecc="998" Category="0" Condition="2" Name="Ambulance" />
    <EccEntry Ecc="112" Category="0" Condition="1" Name="Emergency Number" />
</EccTable>
