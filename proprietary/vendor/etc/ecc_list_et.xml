<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Ethiopia
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="911" Category="0" Condition="2" Name="Emergency Number" />
    <EccEntry Ecc="912" Category="0" Condition="2" Name="Emergency Number" />
    <EccEntry Ecc="915" Category="0" Condition="2" Name="Emergency Number" />
    <EccEntry Ecc="905" Category="0" Condition="2" Name="Emergency Number" />
    <EccEntry Ecc="906" Category="0" Condition="2" Name="Emergency Number" />
    <EccEntry Ecc="907" Category="0" Condition="2" Name="Ambulance" />
    <EccEntry Ecc="952" Category="0" Condition="2" Name="Emergency Number" />
    <EccEntry Ecc="939" Category="0" Condition="2" Name="Fire" />
    <EccEntry Ecc="991" Category="0" Condition="2" Name="Police" />
</EccTable>
