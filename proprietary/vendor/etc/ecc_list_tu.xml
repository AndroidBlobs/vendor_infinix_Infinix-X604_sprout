<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Tunisia
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="190" Category="0" Condition="2" Name="SAMU" />
    <EccEntry Ecc="197" Category="0" Condition="2" Name="Police Secours" />
    <EccEntry Ecc="198" Category="0" Condition="2" Name="Protection Civile" />
    <EccEntry Ecc="193" Category="0" Condition="2" Name="Garde Nationale Tunisienne" />
</EccTable>
