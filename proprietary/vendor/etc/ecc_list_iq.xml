<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Iraq
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="104" Category="0" Condition="2" Name="Police"/>
    <EccEntry Ecc="105" Category="0" Condition="2" Name="Police"/>
    <EccEntry Ecc="115" Category="0" Condition="2" Name="Fire"/>
    <EccEntry Ecc="122" Category="0" Condition="2" Name="Ambulance"/>
    <EccEntry Ecc="01130" Category="0" Condition="2" Name="Hot Line"/>
</EccTable>
