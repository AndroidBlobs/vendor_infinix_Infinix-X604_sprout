<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!-- 
        Russia
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="1" Name="Emergency Number"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Name="Emergency Number"/>
    <EccEntry Ecc="101" Category="0" Condition="2" Name="Fire"/>
    <EccEntry Ecc="102" Category="0" Condition="2" Name="Police"/>
    <EccEntry Ecc="103" Category="0" Condition="2" Name="Ambulance"/>
    <EccEntry Ecc="104" Category="0" Condition="2" Name="Gas"/>
</EccTable>
