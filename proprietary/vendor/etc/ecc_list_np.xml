<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
	    Nepal
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="100" Category="0" Condition="2" Name="Police"/>
    <EccEntry Ecc="101" Category="0" Condition="2" Name="Fire Brigade"/>
    <EccEntry Ecc="102" Category="0" Condition="2" Name="Ambulance"/>
    <!-- TSD:modify RALHSJS-512 by bingwei.han 20190509 start -->
    <EccEntry Ecc="911" Category="0" Condition="1" Name="Emergency Number"/>
    <!-- TSD:modify RALHSJS-512 by bingwei.han 20190509 end   -->
    <EccEntry Ecc="103" Category="0" Condition="2" Name="Traffic Police"/>
	<EccEntry Ecc="112" Category="0" Condition="1" Name="Emergency Number"/>
</EccTable>
