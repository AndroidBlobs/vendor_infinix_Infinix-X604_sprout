    <EccEntry Ecc="114" Category="0" Condition="2" Name="Police of Protection of the mores" />
    <EccEntry Ecc="115" Category="0" Condition="2" Name="Anti-Terrorist Police" />
    <EccEntry Ecc="117" Category="0" Condition="2" Name="Road safety police" />
    <EccEntry Ecc="118" Category="0" Condition="2" Name="Road safety police" />
    <EccEntry Ecc="119" Category="0" Condition="2" Name="Police against drug" />
    <EccEntry Ecc="110" Category="0" Condition="2" Name="Maritime Authority" />
    <EccEntry Ecc="911" Category="0" Condition="1" Name="Emergency Number" />
    <EccEntry Ecc="999" Category="0" Condition="1" Name="Emergency Number" />
</EccTable>
