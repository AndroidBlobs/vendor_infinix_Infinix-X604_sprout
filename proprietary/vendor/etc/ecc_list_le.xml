<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        lebanon
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="2" Name="Emergency Number"/>
    <EccEntry Ecc="125" Category="0" Condition="2" Name="Civil Defense"/>
    <EccEntry Ecc="175" Category="0" Condition="2" Name="Fire Department"/>
    <EccEntry Ecc="140" Category="0" Condition="2" Name="Red Cross-Lebanon"/>
    <EccEntry Ecc="1717" Category="0" Condition="2" Name="General Security"/>
</EccTable>
